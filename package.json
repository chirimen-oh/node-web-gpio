{
  "name": "node-web-gpio",
  "version": "1.1.34",
  "description": "GPIO access with Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chirimen-oh/node-web-gpio.git"
  },
  "author": "Kohei Watanabe <kou029w@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "22.18.13",
    "husky": "9.1.7",
    "lint-staged": "15.5.2",
    "prettier": "3.6.2",
    "typedoc": "0.28.14",
    "typescript": "5.9.3"
  },
  "scripts": {
    "build": "tsc",
    "lint": "biome lint index.ts",
    "prepare": "husky install && rm -rf dist && npm run build",
    "precommit": "lint-staged",
    "preversion": "npm run docs && git add docs",
    "docs": "npm run remove:docs && npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./ README.md",
    "typedoc": "typedoc",
    "remove:docs": "rm -rf docs"
  },
  "keywords": [
    "gpio",
    "hardware",
    "iot",
    "linux",
    "pi",
    "raspberry",
    "raspberry pi",
    "raspi",
    "robot",
    "robotics",
    "robots",
    "rpi"
  ],
  "packageManager": "npm@11.6.2"
}
